{
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.eol": "\n",
    "search.exclude": {
      "**/dist": true,
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/manifest.json"
            ],
            "url": "http://json.schemastore.org/chrome-manifest"
        }
    ]
}
