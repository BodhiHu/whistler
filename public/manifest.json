{
  "manifest_version": 2,

  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "1.0.0",
  "default_locale": "zh_CN",

  "browser_action": {
    "default_icon": "logo-off.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },

  "background": {
    "scripts": ["js/background.js"]
  },

  "permissions": [
    "storage",
    "<all_urls>",
    "tabs",
    "proxy"
  ]
}
